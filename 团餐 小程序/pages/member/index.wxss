/* pages/member/index.wxss */

.user-info-group.com-cell-group .com-cell {
  padding: 40rpx 30rpx;
  background: url('data:image/png;base64,') no-repeat center / 100%;
}

.user-info-group .com-cell .com-cell-head image {
  width: 120rpx;
  height: 120rpx;
  /* border: 2px solid #fff; */
  margin-right: 20rpx;
  border-radius: 50%;
}

.user-info-group .com-cell .com-cell-body {
  font-size: 30rpx;
  font-weight: 400;
  color: rgba(11, 13, 44, 1);
}

.list-group .com-cell .com-cell-head image {
  width: 48rpx;
  height: 48rpx;
  margin-right: 16rpx;
}

.list-group .com-cell {
  position: relative;
}

.list-group .com-cell::before {
  content: " ";
  position: absolute;
  pointer-events: none;
  box-sizing: border-box;
  -webkit-transform-origin: center;
  transform-origin: center;
  top: auto;
  left: 0;
  right: 0;
  bottom: 0;
  -webkit-transform: scaleY(0.5);
  transform: scaleY(0.5);
  border-bottom: 1px solid #eee;
}

.list-group .com-cell .com-cell-body {
  font-size: 28rpx;
  font-weight: 400;
  color: rgba(84, 85, 107, 1);
}

.list-group .com-cell .com-cell-body .contact {
  border: 0;
  background: #fff;
  width: 100%;
  font-size: 28rpx;
  font-weight: 400;
  color: rgba(84, 85, 107, 1);
  padding: 0;
  margin: 0;
  line-height: normal;
  text-align: left;
}

.list-group .com-cell .com-cell-body .contact::after {
  display: none;
}
